{
  "name": "freefire-likes-api",
  "version": "1.0.0",
  "description": "API de Likes para Free Fire com geração de API Keys",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "setup": "node scripts/setup.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "clean:logs": "node scripts/clean-logs.js",
    "backup:db": "node scripts/backup-db.js",
    "pm2:start": "pm2 start server.js --name freefire-api",
    "pm2:stop": "pm2 stop freefire-api",
    "pm2:restart": "pm2 restart freefire-api",
    "pm2:logs": "pm2 logs freefire-api"
  },
  "keywords": [
    "free-fire",
    "api",
    "likes",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
